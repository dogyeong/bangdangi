  <div>
    <header>
      <div id="header_inner">
        <div class="back_btn">
          <a href="https://bangdangi.web.app/board/list/<%=univ%>">
            <i class="xi-arrow-left"></i>
          </a>
        </div>
        <ul class="share_container">
          
        </ul>
      </div>
    </header>
    <div class="wrapper">
      <form action="/board/create_process" method="post">
        <!-- blcok 1-->
        <div class="block1">
          <div class="univ">
            <!-- 
                    <%= (univ==='cuk') ? '카톨릭대학교' : '' %>   
                    <%= (univ==='uos') ? '서울시립대학교' : '' %>
                    <%= (univ==='skku') ? '성균관대학교' : '' %> 
                  -->
            <%= (univ==='hufsSC') ? '한국외국어대학교 서울캠퍼스' : '' %>
            <%= (univ==='khuSC') ? '경희대학교 서울캠퍼스' : '' %>
            <%= (univ==='hanyang') ? '한양대학교' : '' %>
            <%= (univ==='cnu') ? '충남대학교' : '' %>
            <%= (univ==='pnu') ? '부산대학교' : '' %>
            <%= (univ==='pknu') ? '부경대/경성대학교' : '' %>
          </div>
          <div class="title">
            <input class="text_input_full" name="title" type="text" placeholder="제목" required>
          </div>
        </div>
        <div class="block_divider"></div>
        <!-- block 2 -->
        <div class="block2">
          <div class="block_t">건물정보</div>
          <!-- 건물위치 -->
          <div class="select_inpt_type_container">
            <div class="select_btn" data-type="locationL">
              <div>건물위치</div>
              <div class="container_toggle_btn"><i class="xi-angle-down-min"></i></div>
            </div>
            <div class="locationL_container toggle hidden">
              <span><input type="checkbox" name="locationL" value="L_a"> L_a</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bggggggggggggg</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bffffffffffff</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_beeeeeeeeeeee</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bbbbbbbbbbbbb</span>
            </div>
          </div>
          <!-- 건물 상세위치 -->    
          <input class="text_input_full" type="text" name="locationS" placeholder="상세위치">
        </div>
        <div class="block_divider"></div>
        <!-- block 3 -->
        <div class="block3">
          <div class="block_t">매물정보</div>
          <!-- 월세 -->
          <div class="enable_input_type_container">
            <input name="price" type="number" placeholder="월세">
            <div class="enable_input_type_end">만원</div>
            <div class="enable_toggle_btn">토글버튼</div>
          </div>
          <!-- 보증금 -->
          <div class="enable_input_type_container">
            <input name="deposit" type="number" placeholder="보증금">
            <div class="enable_input_type_end">만원</div>
            <div class="enable_toggle_btn">토글버튼</div>
          </div>
          <!-- 관리비 -->
          <div class="enable_input_type_container">
            <input name="expense" type="number" placeholder="관리비">
            <div class="enable_input_type_end">만원</div>
            <div class="enable_toggle_btn">토글버튼</div>
          </div>
          <!-- 가격키워드 -->
          <div class="select_inpt_type_container">
            <div class="select_btn" data-type="keywords">
              <div>가격키워드</div>
              <div class="container_toggle_btn"><i class="xi-angle-down-min"></i></div>
            </div>
            <div class="keywords_container toggle hidden">
              <span><input type="checkbox" name="locationL" value="L_a"> L_a</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bggggggggggggg</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bffffffffffff</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_beeeeeeeeeeee</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bbbbbbbbbbbbb</span>
            </div>
          </div>
          <!-- 기간키워드 -->
          <div class="select_inpt_type_container">
            <div class="select_btn" data-type="dateKeywords">
              <div>건물위치</div>
              <div class="container_toggle_btn"><i class="xi-angle-down-min"></i></div>
            </div>
            <div class="dateKeywords_container toggle hidden">
              <span><input type="checkbox" name="locationL" value="L_a"> L_a</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bggggggggggggg</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bffffffffffff</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_beeeeeeeeeeee</span>
              <span><input type="checkbox" name="locationL" value="L_b"> L_bbbbbbbbbbbbb</span>
