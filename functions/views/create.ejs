<script>
  var btn = document.querySelectorAll('.container_toggle_btn');
  
  btn.forEach((b) => {
    b.addEventListener('click', function() {
      var type = this.parentNode.dataset.field;
      var target = document.querySelector(`.${type}_container`);

      target.classList.toggle('hidden');
      this.firstElementChild.classList.toggle('xi-angle-down-min');
      this.firstElementChild.classList.toggle('xi-angle-up-min');
    });
  });

  document.querySelectorAll('.enable_toggle_btn').forEach((btn) => {
    btn.addEventListener('click', function() {
      let field = this.dataset.field;
      let input = document.querySelector(`input[data-field='${field}']`);
      if (this.classList.toggle('active')) {
        input.disabled = false;
      } else {
        input.value = '';
        input.disabled = true;
      }
    })
  })
</script>
