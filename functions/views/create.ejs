    <%- include('templates/gnb') %>

    <section class="wrapper">

        <h1>매물등록</h1>

        <form class="form-horizontal" action="/board/create_process" method="post" enctype="multipart/form-data">

            <!-- block 1 -->
            <div class="block1 block">
                <!-- 방 사진 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="images">방 사진</label>
                    <div class="col-sm-10">
                        <input type="file" class="form-control" name="images[]" accept="image/*" id="images" multiple/>
                    </div>
                </div>
                <!-- 주소 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="roadFullAddr">주소</label>
                    <div class="col-sm-10">
                        <div class="input-group">
                            <input type="text" class="form-control" name="roadFullAddr" placeholder="검색 버튼을 눌러 입력해주세요" autocomplete="off" required>
                            <span class="input-group-btn">
                                <button class="btn btn-default" type="button" onclick="goPopup();">검색</button>
                            </span>
                        </div>
                    </div>
                </div>
                <!-- hidden 주소 -->
                <input type="hidden" name="roadAddrPart">   <!-- 도로명주소 -->
                <input type="hidden" name="addrDetail">     <!-- 사용자입력정보 -->     
                <input type="hidden" name="siNm">           <!-- 시도명 -->
                <input type="hidden" name="sggNm">          <!-- 시군구명 -->
                <input type="hidden" name="emdNm">          <!-- 읍면동명 -->
                <input type="hidden" name="roadNm">         <!-- 도로명 -->
                <input type="hidden" name="buldMnnm">       <!-- 건물본번 -->
                <input type="hidden" name="buldSlno">       <!-- 건물부번 (없으면 0) -->
                
                    <!-- 가격 -->
                    <div class="form-group">
                        <label class="col-sm-2 control-label" for="deposit">가격</label>
                        <!-- 보증금 -->
                        <div class="col-sm-3">
                            <div class="input-group">
                                <input type="number" class="form-control" name="deposit" placeholder="보증금"
                                    aria-describedby="price-addon" required>
                                <span class="input-group-addon" id="price-addon">만원</span>
                            </div>
                        </div>
                        <!-- 월세 -->
                        <div class="col-sm-3">
                            <div class="input-group">
                                <input type="number" class="form-control" name="price" placeholder="월세"
                                    aria-describedby="deposit-addon" required>
                                <span class="input-group-addon" id="deposit-addon">만원</span>
                            </div>
                        </div>
                        <!-- 관리비 -->
                        <div class="col-sm-3">
                            <div class="input-group">
                                <input type="number" class="form-control" name="expense" placeholder="관리비"
                                    aria-describedby="expense-addon" required>
                                <span class="input-group-addon" id="expense-addon">만원</span>
                            </div>
                        </div>
                    </div>
               
            </div>
            

            <!-- block 2 -->
            <div class="block2 block">
                <h3 class="block-title">계약기간</h3>
                <!-- 최소 계약기간 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="minTerm">
                        최소 계약기간 <input type="radio" name="termType" value="min" checked>
                    </label>
                    <div class="col-sm-10">
                        <div class="input-group">
                            <input type="number" class="form-control" name="minTerm" placeholder="최소 계약기간"
                                aria-describedby="minTerm-addon" required>
                            <span class="input-group-addon" id="minTerm-addon">달 이상</span>
                        </div>
                    </div>
                </div>
                <!-- 고정 계약기간 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="startDate">
                        고정 계약기간 <input type="radio" name="termType" value="fix">
                    </label>
                    <!-- 시작날짜 -->
                    <div class="col-sm-5">
                        <div class="input-group">
                            <input type="date" class="form-control" name="startDate" placeholder="시작날짜"
                                aria-describedby="startDate-addon" disabled>
                            <span class="input-group-addon" id="startDate-addon">부터</span>
                        </div>
                    </div>
                    <!-- 끝 날짜 -->
                    <div class="col-sm-5">
                        <div class="input-group">
                            <input type="date" class="form-control" name="endDate" placeholder="끝날짜"
                                aria-describedby="endDate-addon" disabled>
                            <span class="input-group-addon" id="endDate-addon">까지</span>
                        </div>
                    </div>
                </div>
            </div>

            <!-- block 3 -->
            <div class="block3 block">
                <h3 class="block-title">상세설명</h3>

                <!-- 거래형태 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="tradeType">거래형태</label>
                    <div class="col-sm-10">
                        <label class="radio-inline">
                            <input type="radio" name="tradeType" value="주인직" checked> 주인직
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="tradeType" value="전대"> 전대
                        </label>
                        <label class="radio-inline">
                            <input type="radio" name="tradeType" value="쉐어"> 쉐어
                        </label>
                    </div>
                </div>

                <!-- 상세설명 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="text">상세설명</label>
                    <div class="col-sm-10">
                        <textarea name="text" class="form-control" rows="5" placeholder="상세설명을 적어주세요"
                            required></textarea>
                    </div>
                </div>

                <!-- 태그 -->
            </div>

            <!-- block 4 -->
            <div class="block4 block">
                <h3 class="block-title">연락처</h3>

                <!-- 연락처 -->
                <div class="form-group">
                    <label class="col-sm-2 control-label" for="contact">연락처</label>
                    <div class="col-sm-10">
                        <input type="number" class="form-control" name="contact" placeholder="휴대폰 번호를 - 제외하고 입력해주세요" required>
                    </div>
                </div>

            </div>

            <!-- 취소 / 제출 -->
            <div class="block5 block">
                <div class="form-group">
                    <input class="form-control input-lg" type="submit" value="등록">
                </div>
            </div>

        </form>

    </section> <!-- End of wrapper -->

    <%- include('templates/footer') %>
</body>
