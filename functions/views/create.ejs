<script>
    var termType = document.querySelectorAll('input[name="termType"]');
    var minTerm = document.querySelector('input[name="minTerm"]');
    var startDate = document.querySelector('input[name="startDate"]');
    var endDate = document.querySelector('input[name="endDate"]');

    termType.forEach(function(input) {
        input.addEventListener('change', function() {
            var isMin = (this.value === 'min');
            
            minTerm.disabled = !isMin;
            startDate.disabled = endDate.disabled = isMin;

            minTerm.required = isMin;
            startDate.required = endDate.required = !isMin;
        });
    }); 

    var addr = document.querySelector('input[name="roadFullAddr"]');

    addr.addEventListener('keydown', function(e) {
        e.preventDefault();
    })
    addr.addEventListener('paste', function(e) {
        e.preventDefault();
    })
</script>
